{
  "name": "salman-gabaev/gabaev-backend-slim-4",
  "description": "Gabaev - Template backend slim 4",
  "version": "1.0.0",
  "type": "project",
  "homepage": "https://github.com/salman-gabaev/gabaev-backend-slim-4",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabaev Salman",
      "email": "salmangabaev@gmail.com",
      "role": "CTO"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "php-di/php-di": "^6.3",
    "slim/psr7": "^1.2",
    "slim/slim": "^4.6",
    "interup-utils-backend/template-backend-slim-4-utils": "v1.4.5",
    "dg/bypass-finals": "^1.3",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "jangregor/phpstan-prophecy": "~0.8",
    "phpstan/extension-installer": "^1.0",
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "^3.3.2",
    "ext-simplexml": "*",
    "infection/infection": "0.25.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/salman-gabaev/gabaev-backend-slim-4.git"
    }
  ],
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Template\\Backend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
